<section class='main'>
    <div class='logo'>
        <div id='needsTooltip'>
            <img src='https://static.vecteezy.com/system/resources/previews/024/521/350/original/skateboard-transparent-background-png.png' alt='Logo' />
        </div>
        <logo-elements-tooltip for='needsTooltip' text='Some Kind of text based tooltip content' position='top'></logo-elements-tooltip>
    </div>
    <article class='login'>
        <h1>SKATE 4</h1>
        <h2>DON'T END UP IN THE HALL OF MEAT</h2>
        <div class='login-card'>
            <h4 class='page-title'>Login</h4>
            <form class='form login-form'>
                <div class='form-group'>
                    <label for='email-login'>Email:</label>
                    <input class='form-input' type='text' id='email-login' />
                </div>
                <div class='form-group'>
                    <label for='password-login'>Password:</label>
                    <input class='form-input' type='password' id='password-login' />
                </div>
                <div class='form-group'>
                    <button class='btn btn-primary' type='submit'>Login/ Sign Up</button>
                </div>
            </form>
        </div>
    </article>
</section>



